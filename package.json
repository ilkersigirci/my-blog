{
	"name": "blog-markdown",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 5173",
		"build": "vite build && pagefind",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@neoconfetti/svelte": "^2.2.1",
		"@playwright/test": "^1.48.0",
		"@sveltejs/adapter-static": "^3.0.5",
		"@sveltejs/adapter-vercel": "^5.4.5",
		"@sveltejs/enhanced-img": "^0.3.8",
		"@sveltejs/kit": "^2.6.4",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@types/eslint": "9.6.0",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"eslint": "^9.12.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.44.1",
		"mdsvex": "^0.11.2",
		"mdsvex-enhanced-images": "^0.2.0",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"shiki-transformer-copy-button": "^0.0.3",
		"svelte": "^4.2.19",
		"svelte-check": "^3.8.6",
		"tslib": "^2.7.0",
		"typescript": "^5.6.3",
		"vite": "^5.4.8",
		"vite-plugin-pagefind": "^0.2.10",
		"vitest": "^2.1.2"
	},
	"dependencies": {
		"@fontsource/fira-mono": "^5.1.0",
		"@fontsource/jetbrains-mono": "^5.1.1",
		"@fontsource/manrope": "^5.1.0",
		"@pagefind/default-ui": "^1.1.1",
		"lucide-svelte": "^0.414.0",
		"open-props": "^1.7.7",
		"pagefind": "^1.1.1",
		"rehype-slug": "^6.0.0",
		"remark-toc": "^9.0.0",
		"remark-unwrap-images": "^4.0.0",
		"shiki": "^1.22.0",
		"svelte-zoo": "^0.4.13"
	}
